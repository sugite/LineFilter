{
  "name": "log-line-filter",
  "displayName": "Log Line Filter",
  "description": "Filter lines with advanced regex patterns, logical expressions, and multi-file support. Perfect for log analysis.",
  "version": "1.1.2",
  "publisher": "Sugite",
  "author": {
    "name": "Sugite"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/sugite/LineFilter.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "keywords": [
    "filter",
    "regex",
    "search",
    "pattern",
    "log",
    "multi-file",
    "highlight"
  ],
  "activationEvents": [
    "onCommand:log-line-filter.filter"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "log-line-filter.filter",
        "title": "Log Line Filter",
        "icon": "$(filter)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "log-line-filter.filter",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./node_modules/mocha/bin/mocha --require ts-node/register ./src/test/**/*.test.ts",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.18.121",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.x.x",
    "@typescript-eslint/parser": "^5.x.x",
    "mocha": "^10.8.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.x.x"
  }
}
