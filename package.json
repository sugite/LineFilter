{
  "name": "log-line-filter",
  "displayName": "Log Line Filter",
  "description": "Filter lines with advanced regex patterns and logical expressions",
  "version": "1.0.0",
  "publisher": "dengbo",
  "author": {
    "name": "dengbo"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/sugite/LineFilter.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "filter",
    "regex",
    "search",
    "pattern",
    "line"
  ],
  "activationEvents": [
    "onCommand:log-line-filter.filter"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "log-line-filter.filter",
        "title": "Log Line Filter",
        "icon": "$(filter)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "log-line-filter.filter",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.x.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.x.x",
    "@typescript-eslint/parser": "^5.x.x",
    "sharp": "^0.33.5",
    "typescript": "^4.x.x"
  },
  "dependencies": {
    "canvas": "^2.11.2"
  }
}
